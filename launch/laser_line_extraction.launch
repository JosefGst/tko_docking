<launch>
  <node name="line_extractor" pkg="laser_line_extraction" type="line_extraction_node">
    <param name="~frequency" value="10.0" />
    <param name="~frame_id" value="robot3/scan" />
    <param name="~scan_topic" value="/robot3/scan_filtered" />
    <param name="~publish_markers" value="true" />
    <!-- <param name="~bearing_std_dev" value="1e-10" />
    <param name="~range_std_dev" value="0.0002" /> -->
    <!-- <param name="~least_sq_angle_thresh" value="0.1" /> -->
    <!-- <param name="~least_sq_radius_thresh" value="0.1" /> -->
    <param name="~max_line_gap" value="0.02" />
    <param name="~min_line_length" value="0.01" />
    <param name="~min_range" value="0.0" />
    <param name="~max_range" value="1.0" />
    <param name="~min_split_dist" value="0.025" />
    <!-- <param name="~outlier_dist" value="0.06" /> -->
    <param name="~min_line_points" value="5" />
  </node>
</launch>

